# A product of my warped mind.
function motivate {
	motivations=(
		"*borat voice* very nice"
		"doing it"
		"electrolytes"
		"so strogn"
		"such greatness"
		"such intensity"
		"t.g.i.$(date +"%A" | head -c 1)"
		"ur the greatset"
		"whos a good boy"
		)

	# DOING IT
	pick_one="$[($RANDOM % ${#motivations[*]}) + 1]"
	echo "\n$(unicorn say ${motivations[$pick_one]})\n"

	unset motivations
	unset pick_one
}

